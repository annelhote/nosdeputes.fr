#!/bin/bash
. bin/db.inc
TMPFILE=/tmp/hard_cache.$$

for url in synthese synthesetri/10 synthesetri/11 synthesetri/12 synthesetri/1 synthesetri/2 synthesetri/3 synthesetri/4 synthesetri/5 synthesetri/6 synthesetri/7 synthesetri/8 synthesetri/9;
do 
  rm web/$url.html
  curl -ksL https://www.nossenateurs.fr/$url > $TMPFILE
  mv $TMPFILE web/$url.html
done;

for seance_id in $(echo "select DISTINCT seance_id from presence order by seance_id" | mysql $MYSQLID $DBNAME | grep -v seance_id )
do
	echo "https://www.nossenateurs.fr/seance/$seance_id"
	rm web/seance/$seance_id.html 2> /dev/null
	curl -ksL https://www.nossenateurs.fr/seance/$seance_id > $TMPFILE
	mv $TMPFILE web/seance/$seance_id.html
	sleep 5
done
rm -f cache/frontend/prod/template/www_nossenateurs_fr/all/intervention/seance/seance/*
rm -f cache/frontend/prod/template/www_nossenateurs_fr/all/sf_cache_partial/plot/_groupes/sf_cache_key/*

